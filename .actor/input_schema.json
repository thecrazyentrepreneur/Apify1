{
  "title": "Influencer Data Scraper Input",
  "type": "object",
  "schemaVersion": 1,
  "properties": {
    "platformLinks": {
      "title": "Platform Links",
      "type": "array",
      "description": "Creator URLs",
      "editor": "stringList",
      "example": ["https://www.instagram.com/shraddha.dsgn/"],
      "prefill": ["https://www.instagram.com/shraddha.dsgn/"]
    },
    "executive": {
      "title": "Executive",
      "type": "string",
      "editor": "textfield",
      "example": "Joyce",
      "prefill": "Joyce"
    },
    "team": {
      "title": "Team",
      "type": "string",
      "editor": "select",
      "enum": ["Warriors", "Falcons", "Phoenix", "Eagles"],
      "prefill": "Warriors"
    },
    "category": {
      "title": "Category",
      "type": "string",
      "editor": "textfield",
      "example": "Tech",
      "prefill": "Tech"
    },
    "internalComment": {
      "title": "Internal Comment",
      "type": "string",
      "editor": "textarea",
      "example": "Quick responder",
      "prefill": "Test run"
    }
  },
  "required": ["platformLinks"]
}
